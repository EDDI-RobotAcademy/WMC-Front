{\n            const replaceV = v.replace(/(\\s*)/g, '')\n            const pattern = /^[0-9a-zA-Z]([-_.]?[0-9a-zA-Z])*@[0-9a-zA-Z]([-_.]?[0-9a-zA-Z])*.[a-zA-Z]{2,3}$/\n            return pattern.test(replaceV) || '이메일 형식으로 입력해주세요'\n          }\n        ],\n        password_rule: [\n          v => this.state === 'ins' ? !!v || '패스워드는 필수 입력사항입니다.' : true,\n          v => !(v && v.length >= 30) || '패스워드는 30자 이상 입력할 수 없습니다.',\n        ],\n      };\n    },\n    methods: {\n      onSubmit() {\n        const { email, password } = this;\n        this.$emit(\"submit\", { email, password });\n      },\n    },\n  };\n  \n  </script>\n  \n  <style scoped>\n  \n  @import url(\"https://fonts.googleapis.com/css2?family=Noto+Sans+KR:wght@300;400&display=swap\");\n  \n  </style>"],"mappings":"AA+EA;EACAA,IAAA;EACAC,KAAA;IACA;MACAC,KAAA;MACAC,QAAA;MACAC,UAAA,GACAC,CAAA,MAAAA,CAAA,oBACAA,CAAA;QACA,MAAAC,QAAA,GAAAD,CAAA,CAAAE,OAAA;QACA,MAAAC,OAAA;QACA,OAAAA,OAAA,CAAAC,IAAA,CAAAH,QAAA;MACA,EACA;MACAI,aAAA,GACAL,CAAA,SAAAM,KAAA,eAAAN,CAAA,gCACAA,CAAA,MAAAA,CAAA,IAAAA,CAAA,CAAAO,MAAA;IAEA;EACA;EACAC,OAAA;IACAC,SAAA;MACA;QAAAZ,KAAA;QAAAC;MAAA;MACA,KAAAY,KAAA;QAAAb,KAAA;QAAAC;MAAA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}