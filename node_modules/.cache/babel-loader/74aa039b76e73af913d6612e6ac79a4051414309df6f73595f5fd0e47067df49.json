{"ast":null,"code":"export default {\n  data() {\n    return {\n      selectAll: false,\n      cartItems: [\n      // Sample data, replace with actual cart items\n      {\n        name: 'Item 1',\n        quantity: 1,\n        price: 100,\n        selected: false\n      }, {\n        name: 'Item 2',\n        quantity: 2,\n        price: 150,\n        selected: false\n      }, {\n        name: 'Item 3',\n        quantity: 3,\n        price: 200,\n        selected: false\n      }],\n      shippingFee: 50 // Example shipping fee\n    };\n  },\n\n  computed: {\n    sum() {\n      return this.cartItems.reduce((acc, item) => acc + (item.selected ? item.quantity * item.price : 0), 0);\n    }\n  },\n  methods: {\n    toggleAllItems() {\n      this.cartItems.forEach(item => item.selected = this.selectAll);\n    },\n    removeSelectedItems() {\n      this.cartItems = this.cartItems.filter(item => !item.selected);\n    }\n  }\n};","map":{"version":3,"names":["data","selectAll","cartItems","name","quantity","price","selected","shippingFee","computed","sum","reduce","acc","item","methods","toggleAllItems","forEach","removeSelectedItems","filter"],"sources":["src/components/order/CartForm.vue"],"sourcesContent":["<template>\n  <div>\n    <h3>Your Cart</h3>\n    <table class=\"table table-bordered\">\n      <thead>\n        <tr>\n          <th>\n            <input type=\"checkbox\" v-model=\"selectAll\" @change=\"toggleAllItems\" />\n          </th>\n          <th>Item</th>\n          <th>Quantity</th>\n          <th>Price</th>\n          <th>Subtotal</th>\n        </tr>\n      </thead>\n      <tbody>\n        <tr v-for=\"(item, index) in cartItems\" :key=\"index\">\n          <td>\n            <input type=\"checkbox\" v-model=\"item.selected\" />\n          </td>\n          <td>{{ item.name }}</td>\n          <td>{{ item.quantity }}</td>\n          <td>{{ item.price }}</td>\n          <td>{{ item.price * item.quantity }}</td>\n        </tr>\n      </tbody>\n    </table>\n    <div>\n      <p>Sum: {{ sum }}</p>\n      <p>Shipping fee: {{ shippingFee }}</p>\n      <p>Total: {{ sum + shippingFee }}</p>\n      <button class=\"btn btn-danger\" @click=\"removeSelectedItems\">Remove Selected Items</button>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  data() {\n    return {\n      selectAll: false,\n      cartItems: [\n        // Sample data, replace with actual cart items\n        { name: 'Item 1', quantity: 1, price: 100, selected: false },\n        { name: 'Item 2', quantity: 2, price: 150, selected: false },\n        { name: 'Item 3', quantity: 3, price: 200, selected: false },\n      ],\n      shippingFee: 50 // Example shipping fee\n    };\n  },\n  computed: {\n    sum() {\n      return this.cartItems.reduce((acc, item) => acc + (item.selected ? item.quantity * item.price : 0), 0);\n    }\n  },\n  methods: {\n    toggleAllItems() {\n      this.cartItems.forEach((item) => (item.selected = this.selectAll));\n    },\n    removeSelectedItems() {\n      this.cartItems = this.cartItems.filter((item) => !item.selected);\n    },\n  },\n};\n</script>\n"],"mappings":"AAqCA;EACAA,KAAA;IACA;MACAC,SAAA;MACAC,SAAA;MACA;MACA;QAAAC,IAAA;QAAAC,QAAA;QAAAC,KAAA;QAAAC,QAAA;MAAA,GACA;QAAAH,IAAA;QAAAC,QAAA;QAAAC,KAAA;QAAAC,QAAA;MAAA,GACA;QAAAH,IAAA;QAAAC,QAAA;QAAAC,KAAA;QAAAC,QAAA;MAAA,EACA;MACAC,WAAA;IACA;EACA;;EACAC,QAAA;IACAC,IAAA;MACA,YAAAP,SAAA,CAAAQ,MAAA,EAAAC,GAAA,EAAAC,IAAA,KAAAD,GAAA,IAAAC,IAAA,CAAAN,QAAA,GAAAM,IAAA,CAAAR,QAAA,GAAAQ,IAAA,CAAAP,KAAA;IACA;EACA;EACAQ,OAAA;IACAC,eAAA;MACA,KAAAZ,SAAA,CAAAa,OAAA,CAAAH,IAAA,IAAAA,IAAA,CAAAN,QAAA,QAAAL,SAAA;IACA;IACAe,oBAAA;MACA,KAAAd,SAAA,QAAAA,SAAA,CAAAe,MAAA,CAAAL,IAAA,KAAAA,IAAA,CAAAN,QAAA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}